
@model VT.Web.Models.SplashCustomerModel

@{
    ViewBag.Title = "Splash";
    Layout = "~/Views/Shared/LoggedInLayout.cshtml";
}

@section PageLevelScripts{

    @Scripts.Render("~/plugins/validate")
    <script src="@Url.Content("~/Scripts/app/splash-customer.js")"></script>

}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Credit Card Processing</h2>
    </div>
    <div class="col-lg-2">
    </div>
</div>

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>SPLASH</h2>
    </div>
    <div class="col-lg-2">

    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>SETUP CUSTOMER</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                     @Html.Partial("SetupSplashCreditCard", Model)
                </div>
            </div>
        </div>
    </div>
</div>