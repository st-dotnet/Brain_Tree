 @model VT.Web.Models.UserCustomerAccessDetailModel

<div id="modal-customer-access-form" class="modal fade" aria-hidden="true">
    <div class="modal-dialog">
        <form id="customerAccessForm" action="@Url.Action("UserCustomerAccess", "Users")" class="form-horizontal" method="POST" role="form">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title">Customer Access</h4>
                    <small class="font-bold"></small>
                    @Html.HiddenFor(x => x.CompanyWorkerUserId)
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-xs-6"><h4>All Customers</h4></div>
                        <div class="col-xs-6"><h4>Accessible Customers</h4></div>
                    </div>
                    <div class="row">
                        <div class="col-xs-4">
                            @Html.DropDownListFor(x => x.From, Model.FromList, new
                            {
                                @class = "form-control",
                                multiple = "multiple",
                                data_right = "#multiselect_to_1",
                                data_right_all = "#right_All_1",
                                data_right_selected = "#right_Selected_1",
                                data_left_selected = "#left_Selected_1",
                                data_left_all = "#left_All_1",
                                size = "10",
                                id = "multiselect_from_1"
                            })
                        </div>
                        <div class="col-xs-2 mt35">
                            <button type="button" id="right_All_1" class="btn btn-block"><i class="fa fa-forward"></i></button>
                            <button type="button" id="right_Selected_1" class="btn btn-default btn-block"><i class="fa fa-chevron-right"></i></button>
                            <button type="button" id="left_Selected_1" class="btn btn-default btn-block"><i class="fa fa-chevron-left"></i></button>
                            <button type="button" id="left_All_1" class="btn btn-block"><i class="fa fa-backward"></i></button>
                        </div>

                        <div class="col-xs-4">
                            @Html.DropDownListFor(x => x.To, Model.ToList, new
                            {
                                @class = "form-control",
                                multiple = "multiple",
                                size = "10",
                                id = "multiselect_to_1"
                            })
                        </div>
                        <div class="col-xs-2 mt35">
                            <button type="button" id="btnUp" class="btn btn-default btn-block"><i class="fa fa-arrow-up"></i></button>
                            <button type="button" id="btnDown" class="btn btn-default btn-block"><i class="fa fa-arrow-down"></i></button>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button id="btnCustomerAccess" type="submit" class="btn btn-warning">Submit</button>
                </div>
            </div>
        </form>
    </div>
</div>
 